<!DOCTYPE html>
<html lang="en">
<head>
    <META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
    <title>Routing</title>
</head>
<body>
    
</body>
<script>
    var device = navigator.userAgent;
    if(device.match(/(iPod|iPhone|iPad)/)){
        alert("ios")
    }
    else if(device.match(/(Android)/)){
        alert("Android")
    }
